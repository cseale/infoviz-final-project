{
  "name": "infoviz-final-project",
  "version": "1.0.0",
  "description": "Data Vizualisation 2018 InfoViz Final Project",
  "main": "index.js",
  "scripts": {
    "start": "npm run start-front && npm run start-back",
    "setup": "node ./setup",
    "start-front": "pm2 start ./node_modules/.bin/http-server --name frontend -- ./dist --proxy=http://localhost:3000",
    "start-back": "pm2 start ./backend-src --name backend",
    "restart": "pm2 restart frontend backend",
    "stop": "pm2 stop frontend backend",
    "delete": "pm2 delete frontend backend",
    "reload": "npm run delete && npm run start",
    "build": "rm -rf dist && ./node_modules/.bin/webpack --mode=development",
    "watch": "rm -rf dist && ./node_modules/.bin/webpack --mode=development --watch",
    "dump": "elasticdump --output=http://localhost:9200/migration_total --input=./mapping.json --type=mapping && elasticdump --output=http://localhost:9200/migration_total --input=./data_latest.json --type=data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cseale/infoviz-final-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cseale/infoviz-final-project/issues"
  },
  "homepage": "https://github.com/cseale/infoviz-final-project#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.5",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "bootstrap-select": "^1.13.3",
    "csvtojson": "^2.0.8",
    "d3": "^5.7.0",
    "d3-geo-projection": "^2.5.0",
    "d3-queue": "^1.2.3",
    "d3-selection": "^1.3.2",
    "d3-tip": "^0.9.1",
    "echarts": "^4.2.0-rc.2",
    "elasticdump": "^4.1.1",
    "elasticsearch": "^15.2.0",
    "express": "^4.16.4",
    "i18n-iso-countries": "^3.7.8",
    "jquery": "^3.3.1",
    "nouislider": "^12.1.0",
    "pm2": "^3.2.2",
    "popper.js": "^1.14.5",
    "topojson": "^3.0.2",
    "wnumb": "^1.1.0"
  }
}
